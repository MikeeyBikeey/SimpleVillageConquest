var y_pos = floor(view_yview[0] + 5);

draw_set_colour(c_white);
draw_set_halign(fa_middle);
draw_set_valign(fa_top);
draw_text(x_pos, y_pos, "Boss Health");

var bar_x = floor(x_pos - sprite_get_width(spr_boss_health) / 2);
var bar_y = y_pos + 20;

draw_sprite(spr_boss_health, 0, bar_x, bar_y);

var bar_width = sprite_get_width(spr_boss_health) - 2;
var bar_height = sprite_get_height(spr_boss_health) - 2;

if alarm[1] == -1 {
    draw_set_alpha(0.5);
    draw_set_colour(c_red);
    draw_rectangle(
        bar_x + 1,
        bar_y + 1,
        bar_x + ((hearts + lhand.hearts + rhand.hearts) / (max_hearts + lhand.max_hearts + rhand.max_hearts) * bar_width),
        bar_y + bar_height,
